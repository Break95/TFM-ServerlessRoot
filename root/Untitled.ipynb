    "import ROOT\n",
    "import numpy as np\n",
    "\n",
    "x = np.array([1,2,3], dtype=np.int32)\n",
    "y = np.array([4,5,6], dtype=np.float64)\n",
    "\n",
    "df = ROOT.RDF.MakeNumpyDataFrame({'x':x , 'y':y})\n",
    "\n",
    "df = df.Define('z', 'x+y')\n",
    "\n",
    "df.Display().Print()\n"
